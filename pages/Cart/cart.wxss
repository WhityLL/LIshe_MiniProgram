/* pages/Cart/cart.wxss */

page{
  background: #f5f5f5;
}
/* 主体 */
.container-view {
  background: #f5f5f5;
  width: 100%;
  height: 100%;
  margin: 90rpx auto;
}

/* 顶部 */
.cart-headerView {
  width: 100%;
  height: 60rpx;
  position: fixed;
  top: 0;
  background: burlywood
}

/* 中间 */
.cart-wrap {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

/* section */
.section {
  width: 100%;
  background: #fff;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
  padding: 4rpx 16rpx;
  box-sizing: border-box;
}

/* sectionHeader 正常的头部 */
.section-header-normal{
  width: 100%;
  height: 80rpx;
  background: blanchedalmond;
}

.cell{
  width: 100%;
  height: 200rpx;
  margin-top: 4rpx;
  background: beige;

}

/* 底部 */
.cart-footer {
  width: 100%;
  height: 90rpx;
  background: orangered;
  position: fixed;
  bottom: 0;
  z-index: 10;
}




